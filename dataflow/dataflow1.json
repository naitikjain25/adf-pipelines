{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "FlipKartCsv",
						"type": "DatasetReference"
					},
					"name": "FlipkartDataSetCsv"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "FlipkartParquet",
						"type": "DatasetReference"
					},
					"name": "Sink"
				}
			],
			"transformations": [
				{
					"name": "filterproducts"
				},
				{
					"name": "derivedColumns"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as string,",
				"          title as string,",
				"          Rating as string,",
				"          maincateg as string,",
				"          platform as string,",
				"          actprice1 as string,",
				"          norating1 as string,",
				"          noreviews1 as string,",
				"          star_5f as string,",
				"          star_4f as string,",
				"          star_3f as string,",
				"          star_2f as string,",
				"          star_1f as string,",
				"          fulfilled1 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> FlipkartDataSetCsv",
				"FlipkartDataSetCsv filter(toInteger(norating1)<10 || toInteger(noreviews1)<10) ~> filterproducts",
				"filterproducts derive(positive_review_ratio = toInteger(star_5f) / toInteger(norating1),",
				"          total_stars = toDouble(star_1f) + toDouble(star_2f) + toDouble(star_3f) + toDouble(star_4f) + toDouble(star_5f)) ~> derivedColumns",
				"derivedColumns sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Sink"
			]
		}
	}
}